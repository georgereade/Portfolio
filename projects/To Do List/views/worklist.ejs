<%- include("./partials/header.ejs") %>
<div class="col-12 align-items-center justify-content-center" id="taskheader"><h2>Work Tasks</h2></div>
<div class="worklist row-col-12 justify-content-center py-3">
    <% if (locals.worktodo) { %>
      <ul class="no-bullets">
        <% worktodo.forEach((worktodo)=> { %>
        <div class="item"><input type="checkbox"><li><%= worktodo %></li></div>
      <%}) %>
      </ul>
      <% } else {%>
        <p>No work to dos! You can relax</p>
    <% } %>
  </div>

  <%- include("./partials/footer.ejs") %>